# platform :ios, '9.0'

target 'CSBC' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CSBC

  pod 'GoogleSignIn'
  pod 'Firebase/Analytics'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Messaging'
  pod 'Fabric'
  pod 'Crashlytics'

  pod 'Alamofire'

  pod 'SwiftSoup', '>=2.0.0'
  pod 'SwiftyJSON'

end